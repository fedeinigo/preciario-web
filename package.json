{
  "name": "preciario-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev --turbopack",
    "build": "next build",
    "build:analyze": "next build --analyze",
    "vercel-build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "typecheck:strict": "tsc --noEmit -p tsconfig.typecheck.json",
    "db:migrate": "prisma migrate dev",
    "db:deploy": "prisma migrate deploy",
    "db:reset": "prisma migrate reset --force",
    "db:studio": "prisma studio",
    "test": "npm run test:unit",
    "test:unit": "node -e \"require('fs').rmSync('.tmp/test-dist', { recursive: true, force: true })\" && tsc -p tsconfig.test.json && node --test \".tmp/test-dist/tests/unit/**/*.js\"",
    "test:e2e": "node -e \"require('fs').rmSync('.tmp/test-dist', { recursive: true, force: true })\" && tsc -p tsconfig.test.json && node --test .tmp/test-dist/tests/e2e"
  },
  "dependencies": {
    "@atlaskit/pragmatic-drag-and-drop": "1.7.7",
    "@atlaskit/pragmatic-drag-and-drop-hitbox": "1.1.0",
    "@auth/prisma-adapter": "^2.10.0",
    "@prisma/client": "^6.16.1",
    "googleapis": "^159.0.0",
    "lucide-react": "^0.542.0",
    "next": "15.5.1",
    "next-auth": "^4.24.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tiny-invariant": "1.3.3",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.5.1",
    "postcss": "^8.5.6",
    "prisma": "^6.15.0",
    "tailwindcss": "^3.4.14",
    "typescript": "^5"
  }
}
